{
  "manifest_version": 3,
  "name": "Domain Swapper",
  "version": "1.0",
  "description": "Swap between predefined domains",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "128": "icon128.png"
    }
  }
}
